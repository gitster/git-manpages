.\}
.nf
git for\-each\-ref \-\-format=\*(Aq%(refname)\*(Aq |
while read branch
do
        size=$(git rev\-list \-\-disk\-usage \-\-objects HEAD\&.\&.$branch)
        echo "$size $branch"
done |
sort \-n
